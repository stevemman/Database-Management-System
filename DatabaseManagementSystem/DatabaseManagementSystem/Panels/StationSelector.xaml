<UserControl x:Class="WPFPageSwitch.StationSelector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        d:DesignWidth="640" d:DesignHeight="480" Width="800" Height="600">
    <Grid Background="{DynamicResource {x:Static SystemColors.MenuBrushKey}}">
        <Button x:Name="button_Laser" Content="Laser" HorizontalAlignment="Left" Margin="280,240,0,0" Width="240" FontFamily="Microsoft JhengHei UI" FontSize="29.333" Click="button_Laser_Click" Height="100" VerticalAlignment="Top"/>
        <Button x:Name="button_3DPrinter" Content="3DPrinter" HorizontalAlignment="Left" Margin="280,400,0,100" Width="240" FontFamily="Microsoft JhengHei UI" FontSize="29.333" Click="button_3DPrinter_Click"/>
        <Button x:Name="button_Back" Content="Back" HorizontalAlignment="Left" Margin="280,80,0,0" VerticalAlignment="Top" Width="240" Height="100" FontFamily="Microsoft JhengHei UI" FontSize="29.333" Click="button_Back_Click"/>
    </Grid>
</UserControl>
